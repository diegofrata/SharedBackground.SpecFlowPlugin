<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>SharedBackground.SpecFlowPlugin</id>
    <version>2.2.2-beta</version>
    <title>SharedBackground.SpecFlowPlugin</title>
    <authors>Diego Frata</authors>
